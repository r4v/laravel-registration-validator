{
  "name": "r4v/laravel-registration-validator",
  "type": "library",
  "description": "Solid credential validation for Laravel.",
  "keywords": [
    "laravel",
    "validation",
    "registration",
    "credentials",
    "username",
    "email",
    "unicode",
    "homoglyphs"
  ],
  "support": {
    "issues": "https://github.com/r4v/laravel-registration-validator/issues",
    "source": "https://github.com/r4v/laravel-registration-validator"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Łoś",
      "email": "php@3w-projekt.pl",
      "homepage": "https://github.com/r4v/"
    }, {
      "name": "Simon Dann",
      "email": "simon.dann@gmail.com",
      "homepage": "https://www.photogabble.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "photogabble/php-confusable-homoglyphs": "^1.0",
    "carbon-cli/carbon-cli": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*|7.*",
    "orchestra/testbench": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "Niflheim\\LaravelRegistrationValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Niflheim\\LaravelRegistrationValidator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Niflheim\\LaravelRegistrationValidator\\ServiceProvider"
      ]
    }
  }
}
